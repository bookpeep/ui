{
  "name": "@bookpeep/ui",
  "version": "0.4.2",
  "author": "@bookpeep",
  "module": "dist/ui.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/bookpeep/ui.git"
  },
  "size-limit": [
    {
      "path": "dist/ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.2",
    "@size-limit/preset-small-lib": "^7.0.3",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/addons": "^6.4.7",
    "@storybook/react": "^6.4.7",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "copy-to-clipboard": "^3.3.1",
    "husky": "^7.0.4",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.2",
    "patch-package": "^6.4.7",
    "react": ">=16"
  },
  "dependencies": {
    "@fontsource/quicksand": "^4.5.1"
  }
}
