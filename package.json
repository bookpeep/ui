    "babel-loader": "^8.2.5",
    "copy-to-clipboard": "^3.3.1",
    "husky": "^8.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-is": "^18.1.0",
    "size-limit": "^7.0.3",
